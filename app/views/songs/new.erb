<h1>Form To Create A New Song:</h1>

<%= form_for(@song) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
  <br>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <br>
  <br>
  <%= f.label :release_year %>
  <%= f.number_field :release_year%>
  <br>
  <br>
  <%= f.check_box :released %>
  <%= f.label :released %>
  <br>
  <br>
  <%= f.submit %>
<% end %>
